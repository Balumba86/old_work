    "paths": {
        "/shop/categories": {
            "get": {
                "tags": [
                    "shop"
                ],
                "summary": "Получение списка категорий магазинов",
                "operationId": "index",
                "responses": {
                    "200": {
                        "description": "Успешный ответ",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CategoryListResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/shop/category/{categorySlug}": {
            "get": {
                "tags": [
                    "shop"
                ],
                "summary": "Получение списка магазинов в категории",
                "operationId": "categoryItems",
                "parameters": [
                    {
                        "name": "categorySlug",
                        "in": "path",
                        "description": "slug запрашиваемой категории",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "search",
                        "in": "query",
                        "description": "Поиск по названию магазина в категории",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Номер запрашиваемой страницы",
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Успешный ответ",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/CategoryShopDetailResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/shop/{shopSlug}": {
            "get": {
                "tags": [
                    "shop"
                ],
                "summary": "Получение детальной информации",
                "operationId": "getItemBySlug",
                "parameters": [
                    {
                        "name": "shopSlug",
                        "in": "path",
                        "description": "slug магазина",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Успешный ответ",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ShopDetailResponse"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
